{
  "name": "camelspace",
  "version": "1.0.0",
  "description": "Transform flat ENVIRONMENT_VARIABLES into deep { environment: { variables } } objects and vice versa.",
  "main": "index.js",
  "files": [
    "index.js"
  ],
  "scripts": {
    "format": "prettier --write \"./*.js\" && eslint --fix ./*.{js,json}",
    "prepublishOnly": "npm run -s test",
    "tap": "tap --cov *.test.js",
    "test": "npm run -s format && npm run -s tap",
    "test:dev": "chokidar --silent --polling --initial \"./*.js\" -c \"npm run -s tap\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zetlen/camelspace.git"
  },
  "keywords": [
    "env",
    "environment",
    "config",
    "namespace",
    "dotenv"
  ],
  "author": "zetlen <zetlen@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zetlen/camelspace/issues"
  },
  "homepage": "https://github.com/zetlen/camelspace#readme",
  "dependencies": {
    "change-case": "^3.1.0"
  },
  "devDependencies": {
    "@magento/eslint-config": "^1.4.0",
    "chokidar-cli": "^1.2.2",
    "eslint": "^5.16.0",
    "eslint-plugin-jsx-a11y": "^6.2.1",
    "eslint-plugin-package-json": "^0.1.3",
    "eslint-plugin-react": "^7.12.4",
    "lodash": "^4.17.11",
    "prettier": "1.16.4",
    "tap": "^12.6.1"
  }
}
